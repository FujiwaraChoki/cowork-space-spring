							"\r",
							"pm.test(\"Response has the required fields\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"\r",
							"    pm.expect(responseData).to.be.an('object');\r",
							"    pm.expect(responseData).to.include.all.keys('id', 'userId', 'bookingDate', 'duration', 'bookingStatus');\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Response body attributes have the specified values\", function () {\r",
							"    const responseData = pm.response.json();\r",
							"\r",
							"    pm.expect(responseData.bookingDate).to.equal('23-04-2024');\r",
							"    pm.expect(responseData.duration).to.equal('MORNING');\r",
							"    pm.expect(responseData.bookingStatus).to.equal('PENDING');\r",
							"});"
